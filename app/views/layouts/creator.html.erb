<!DOCTYPE html>
<html>
  <head>
    <title>CreatedToSell</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <h1>CreatedToSell</h1>>
    <ul class = "nav">
      <li><%= link_to "My Storefront", creator_path(current_creator) %>
      <li><%= link_to "New Listing", new_creator_merchandise_path(current_creator) %>
      <li><%= link_to "Sales", creator_sales_path(current_creator) %>
      <li><%= link_to "Logout", logout_path %>
    </ul>
   
    <%= yield %>
    
  </body>
</html>
